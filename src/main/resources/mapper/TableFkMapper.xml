ltType="com.cyber.domain.entity.TableFk" parameterType="com.cyber.domain.entity.TableFk">        SELECT        <include refid="baseColumns"/>        FROM table_fk        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>    </select>    <select id="selectByIndexCount" parameterType="com.cyber.domain.entity.TableFk" resultType="int">        SELECT count(1)        FROM table_fk        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>    </select>    <select id="selectByIndex" parameterType="com.cyber.domain.entity.TableFk"            resultType="com.cyber.domain.entity.TableFk">        SELECT        <include refid="baseColumns"/>        FROM table_fk        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>        <if test="sortBy != null and sortBy != '' and sortType != null and sortType != '' ">            ORDER BY ${sortBy} ${sortType}        </if>        LIMIT #{offset} , #{limit}    </select>    <select id="selectList" resultType="com.cyber.domain.entity.TableFk">        SELECT        <include refid="baseColumns"/>        FROM table_fk        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>        <if test="sortBy != null and sortBy != '' and sortType != null and sortType != '' ">            ORDER BY ${sortBy} ${sortType}        </if>    </select></mapper>