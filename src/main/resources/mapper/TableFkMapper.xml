<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.cyber.basedata.domain.repository.TableFkMapper">    <sql id="baseColumns">        id        , table_code, name, column_code, `update`, `delete`, cl_table_code, cl_column_code, description, deleted, creator, create_time, updator, update_time, remark    </sql>    <sql id="updateColumns">        <set>            <if test="id != null">                id = #{id},            </if>            <if test="tableCode != null">                table_code = #{tableCode},            </if>            <if test="name != null">                name = #{name},            </if>            <if test="columnCode != null">                column_code = #{columnCode},            </if>            <if test="update != null">                `update` = #{update},            </if>            <if test="delete != null">                `delete` = #{delete},            </if>            <if test="clTableCode != null">                cl_table_code = #{clTableCode},            </if>            <if test="clColumnCode != null">                cl_column_code = #{clColumnCode},            </if>            <if test="description != null">                description = #{description},            </if>            <if test="deleted != null">                deleted = #{deleted},            </if>            <if test="creator != null">                creator = #{creator},            </if>            <if test="createTime != null">                create_time = #{createTime},            </if>            <if test="updator != null">                updator = #{updator},            </if>            <if test="updateTime != null">                update_time = #{updateTime},            </if>            <if test="remark != null">                remark = #{remark},            </if>        </set>    </sql>    <sql id="whereClause">        <trim prefix="WHERE" prefixOverrides="AND">            <if test="id != null">                AND ${columnPrefix}id = #{id}            </if>            <if test="tableCode != null">                AND ${columnPrefix}table_code = #{tableCode}            </if>            <if test="name != null">                AND ${columnPrefix}name = #{name}            </if>            <if test="columnCode != null">                AND ${columnPrefix}column_code = #{columnCode}            </if>            <if test="update != null">                AND `${columnPrefix}update` = #{update}            </if>            <if test="delete != null">                AND `${columnPrefix}delete` = #{delete}            </if>            <if test="clTableCode != null">                AND ${columnPrefix}cl_table_code = #{clTableCode}            </if>            <if test="clColumnCode != null">                AND ${columnPrefix}cl_column_code = #{clColumnCode}            </if>            <if test="description != null">                AND ${columnPrefix}description = #{description}            </if>            <if test="deleted != null">                AND ${columnPrefix}deleted = #{deleted}            </if>            <if test="creator != null">                AND ${columnPrefix}creator = #{creator}            </if>            <if test="createTime != null">                AND ${columnPrefix}create_time = #{createTime}            </if>            <if test="updator != null">                AND ${columnPrefix}updator = #{updator}            </if>            <if test="updateTime != null">                AND ${columnPrefix}update_time = #{updateTime}            </if>            <if test="remark != null">                AND ${columnPrefix}remark = #{remark}            </if>        </trim>    </sql>    <insert id="save" parameterType="com.cyber.domain.entity.TableFk">        insert into table_fk        (<include refid="saveColumn"/>)        values        (<include refid="saveValue"/>)    </insert>    <insert id="saveBatch">        insert into table_fk        (id, table_code, name, column_code, `update`, `delete`, cl_table_code, cl_column_code, description, deleted)        values        <foreach collection="fkList" separator=") , (" item="item" open="(" close=")">            #{item.id},#{item.tableCode},#{item.name},#{item.columnCode},#{item.update},#{item.delete},#{item.clTableCode},#{item.clColumnCode},#{item.description},#{item.deleted}        </foreach>    </insert>    <sql id="saveColumn">        <trim suffixOverrides=",">            <if test="id!= null">id,</if>            <if test="tableCode!= null">table_code,</if>            <if test="name!= null">name,</if>            <if test="columnCode!= null">column_code,</if>            <if test="update!= null">`update`,</if>            <if test="delete!= null">`delete`,</if>            <if test="clTableCode!= null">cl_table_code,</if>            <if test="clColumnCode!= null">cl_column_code,</if>            <if test="description!= null">description,</if>            <if test="deleted!= null">deleted,</if>            <if test="creator!= null">creator,</if>            <if test="createTime!= null">create_time,</if>            <if test="updator!= null">updator,</if>            <if test="updateTime!= null">update_time,</if>            <if test="remark!= null">remark,</if>        </trim>    </sql>    <sql id="saveBatchValue">        <trim suffixOverrides=",">            <if test="item.id!= null">#{item.id},</if>            <if test="item.tableCode!= null">#{item.tableCode},</if>            <if test="item.name!= null">#{item.name},</if>            <if test="item.columnCode!= null">#{item.columnCode},</if>            <if test="item.update!= null">#{item.update},</if>            <if test="item.delete!= null">#{item.delete},</if>            <if test="item.clTableCode!= null">#{item.clTableCode},</if>           