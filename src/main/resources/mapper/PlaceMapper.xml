<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.cyber.basedata.domain.repository.PlaceMapper">	<sql id="baseColumns">		 id, type, rail_type, rail_km, address, lon, lat, rail_lon_lat, zip_code, deleted, creator, create_time, updator, update_time, remark	</sql>	<sql id="updateColumns">		<set>			  <if test="id != null">	id   = #{id},  </if>  <if test="type != null">	type   = #{type},  </if>  <if test="railType != null">	rail_type   = #{railType},  </if>  <if test="railKm != null">	rail_km   = #{railKm},  </if>  <if test="address != null">	address   = #{address},  </if>  <if test="lon != null">	lon   = #{lon},  </if>  <if test="lat != null">	lat   = #{lat},  </if>  <if test="railLonLat != null">	rail_lon_lat   = #{railLonLat,javaType=com.alibaba.fastjson.JSONArray,jdbcType=VARCHAR,typeHandler=com.cyber.infrastructure.extension.JsonArrayHandler},  </if>  <if test="zipCode != null">	  zip_code  = #{zipCode},  </if>  <if test="deleted != null">	deleted   = #{deleted},  </if>  <if test="creator != null">	creator   = #{creator},  </if>  <if test="createTime != null">	create_time   = #{createTime},  </if>  <if test="updator != null">	updator   = #{updator},  </if>  <if test="updateTime != null">	update_time   = #{updateTime},  </if>  <if test="remark != null">	remark   = #{remark},  </if>		</set>	</sql>    <sql id="whereClause">        <trim prefix="WHERE" prefixOverrides="AND">			  <if test="id != null">	    AND ${columnPrefix}id = #{id}  </if>  <if test="type != null">	    AND ${columnPrefix}type = #{type}  </if>  <if test="railType != null">	    AND ${columnPrefix}rail_type = #{railType}  </if>  <if test="railKm != null">	    AND ${columnPrefix}rail_km = #{railKm}  </if>  <if test="address != null">	    AND ${columnPrefix}address = #{address}  </if>  <if test="lon != null">	    AND ${columnPrefix}lon = #{lon}  </if>  <if test="lat != null">	    AND ${columnPrefix}lat = #{lat}  </if>  <if test="railLonLat != null">	    AND ${columnPrefix}rail_lon_lat = #{railLonLat}  </if>  <if test="zipCode != null">	    AND ${columnPrefix}zip_code = #{zipCode}  </if>  <if test="deleted != null">	    AND ${columnPrefix}deleted = #{deleted}  </if>  <if test="creator != null">	    AND ${columnPrefix}creator = #{creator}  </if>  <if test="createTime != null">	    AND ${columnPrefix}create_time = #{createTime}  </if>  <if test="updator != null">	    AND ${columnPrefix}updator = #{updator}  </if>  <if test="updateTime != null">	    AND ${columnPrefix}update_time = #{updateTime}  </if>  <if test="remark != null">	    AND ${columnPrefix}remark = #{remark}  </if>        </trim>    </sql>	<resultMap id="placeResult" type="com.cyber.domain.entity.Place">		<id column="id" property="id"/>		<result column="rail_lon_lat" property="railLonLat" javaType="com.alibaba.fastjson.JSONArray" jdbcType="VARCHAR"				typeHandler="com.cyber.infrastructure.extension.JsonArrayHandler"/>	</resultMap>	<insert id="save" parameterType="com.cyber.domain.entity.Place">		insert into place		(<include refid="saveColumn"/>)		values		(<include refid="saveValue"/>)	</insert>	<sql id="saveColumn">		<trim suffixOverrides=",">			<if test="id!= null">id,</if><if test="type!= null">type,</if><if test="railType!= null">rail_type,</if><if test="railKm!= null">rail_km,</if><if test="address!= null">address,</if><if test="lon!= null">lon,</if><if test="lat!= null">lat,</if><if test="railLonLat!= null">rail_lon_lat,</if><if test="zipCode!= null">zip_code,</if><if test="deleted!= null">deleted,</if><if test="creator!= null">creator,</if><if test="createTime!= null">create_time,</if><if test="updator!= null">updator,</if><if test="updateTime!= null">update_time,</if><if test="remark!= null">remark,</if>		</trim>	</sql>	<sql id="saveValue">		<trim suffixOverrides=",">			<if test="id!= null">#{id},</if><if test="type!= null">#{type},</if><if test="railType!= null">#{railType},</if><if test="railKm!= null">#{railKm},</if><if test="address!= null">#{address},</if><if test="lon!= null">#{lon},</if><if test="lat!= null">#{lat},</if><if test="railLonLat!= null">#{railLonLat,javaType=com.alibaba.fastjson.JSONArray,jdbcType=VARCHAR,typeHandler=com.cyber.infrastructure.extension.JsonArrayHandler},</if><if test="zipCode!= null">#{zipCode},</if><if test="deleted!= null">#{deleted},</if><if test="creator!= null">#{creator},</if><if test="createTime!= null">#{createTime},</if><if test="updator!= null">#{updator},</if><if test="updateTime!= null">#{updateTime},</if><if test="remark!= null">#{remark},</if>		</trim>	</sql>	<delete id="deleteById" parameterType="com.cyber.domain.entity.Place">		DELETE FROM place WHERE id = #{id}	</delete>	<update id="updateById" parameterType="com.cyber.domain.entity.Place">		update place		<include refid="updateColumns" />		where id = #{id}	</update>	<select id="selectOne" resultMap="placeResult" parameterType="com.cyber.domain.entity.Place">		SELECT		<include refid="baseColumns" />		FROM place        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>	</select>	<select id="selectByIndexCount" parameterType="com.cyber.domain.entity.Place" resultType="int">		SELECT count(1)		FROM place        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>	</select>	<select id="selectByIndex" parameterType="com.cyber.domain.entity.Place" resultMap="placeResult">		SELECT		<include refid="baseColumns" />		FROM place        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>		<if test="sortBy != null and sortBy != '' and sortType != null and sortType != '' ">			ORDER BY ${sortBy} ${sortType}		</if>		LIMIT #{offset} , #{limit}	</select>	<select id="selectPlace" resultMap="placeResult" >		SELECT		<include refid="baseColumns" />		FROM place		<include refid="whereClause">			<property name="columnPrefix" value=""/>		</include>		<if test="sortBy != null and sortBy != '' and sortType != null and sortType != '' ">			ORDER BY ${sortBy} ${sortType}		</if>	</select></mapper>