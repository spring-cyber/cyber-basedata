<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.cyber.basedata.domain.repository.TableColumnMapper">	<sql id="baseColumns">		 id, table_code, type, name, code, default_val, order_num, length, cl_table_code, cl_column_code, primary_key, auto_increment, not_null, description, deleted, creator, create_time, updator, update_time, remark	</sql>	<sql id="updateColumns">		<set>			  <if test="id != null">	id   = #{id},  </if>  <if test="tableCode != null">	table_code   = #{tableCode},  </if>  <if test="type != null">	type   = #{type},  </if>  <if test="name != null">	name   = #{name},  </if>  <if test="code != null">	code   = #{code},  </if>  <if test="defaultVal != null">	default_val   = #{defaultVal},  </if>  <if test="orderNum != null">	order_num   = #{orderNum},  </if>  <if test="length != null">	length   = #{length},  </if>  <if test="clTableCode != null">	cl_table_code   = #{clTableCode},  </if>  <if test="clColumnCode != null">	cl_column_code   = #{clColumnCode},  </if>  <if test="primaryKey != null">	primary_key   = #{primaryKey},  </if>  <if test="autoIncrement != null">	auto_increment   = #{autoIncrement},  </if>  <if test="notNull != null">	not_null   = #{notNull},  </if>  <if test="description != null">	description   = #{description},  </if>  <if test="deleted != null">	deleted   = #{deleted},  </if>  <if test="creator != null">	creator   = #{creator},  </if>  <if test="createTime != null">	create_time   = #{createTime},  </if>  <if test="updator != null">	updator   = #{updator},  </if>  <if test="updateTime != null">	update_time   = #{updateTime},  </if>  <if test="remark != null">	remark   = #{remark},  </if>		</set>	</sql>    <sql id="whereClause">        <trim prefix="WHERE" prefixOverrides="AND">			  <if test="id != null">	    AND ${columnPrefix}id = #{id}  </if>  <if test="tableCode != null">	    AND ${columnPrefix}table_code = #{tableCode}  </if>  <if test="type != null">	    AND ${columnPrefix}type = #{type}  </if>  <if test="name != null">	    AND ${columnPrefix}name = #{name}  </if>  <if test="code != null">	    AND ${columnPrefix}code = #{code}  </if>  <if test="defaultVal != null">	    AND ${columnPrefix}default_val = #{defaultVal}  </if>  <if test="orderNum != null">	    AND ${columnPrefix}order_num = #{orderNum}  </if>  <if test="length != null">	    AND ${columnPrefix}length = #{length}  </if>  <if test="clTableCode != null">	    AND ${columnPrefix}cl_table_code = #{clTableCode}  </if>  <if test="clColumnCode != null">	    AND ${columnPrefix}cl_column_code = #{clColumnCode}  </if>  <if test="primaryKey != null">	    AND ${columnPrefix}primary_key = #{primaryKey}  </if>  <if test="autoIncrement != null">	    AND ${columnPrefix}auto_increment = #{autoIncrement}  </if>  <if test="notNull != null">	    AND ${columnPrefix}not_null = #{notNull}  </if>  <if test="description != null">	    AND ${columnPrefix}description = #{description}  </if>  <if test="deleted != null">	    AND ${columnPrefix}deleted = #{deleted}  </if>  <if test="creator != null">	    AND ${columnPrefix}creator = #{creator}  </if>  <if test="createTime != null">	    AND ${columnPrefix}create_time = #{createTime}  </if>  <if test="updator != null">	    AND ${columnPrefix}updator = #{updator}  </if>  <if test="updateTime != null">	    AND ${columnPrefix}update_time = #{updateTime}  </if>  <if test="remark != null">	    AND ${columnPrefix}remark = #{remark}  </if>        </trim>    </sql>	<insert id="save" parameterType="com.cyber.domain.entity.TableColumn">		insert into table_column		(<include refid="saveColumn"/>)		values		(<include refid="saveValue"/>)	</insert>    <insert id="saveBatch">        insert into table_column        (id, table_code, type, name, code, default_val, order_num, length, cl_table_code, cl_column_code, primary_key, auto_increment, not_null, description, deleted)        values        <foreach collection="columnList" separator="," item="item">            (<include refid="saveBatchValue"/>)        </foreach>    </insert>    <sql id="saveColumn">		<trim suffixOverrides=",">			<if test="id!= null">id,</if><if test="tableCode!= null">table_code,</if><if test="type!= null">type,</if><if test="name!= null">name,</if><if test="code!= null">code,</if><if test="defaultVal!= null">default_val,</if><if test="orderNum!= null">order_num,</if><if test="length!= null">length,</if><if test="clTableCode!= null">cl_table_code,</if><if test="clColumnCode!= null">cl_column_code,</if><if test="primaryKey!= null">primary_key,</if><if test="autoIncrement!= null">auto_increment,</if><if test="notNull!= null">not_null,</if><if test="description!= null">description,</if><if test="deleted!= null">deleted,</if><if test="creator!= null">creator,</if><if test="createTime!= null">create_time,</if><if test="updator!= null">updator,</if><if test="updateTime!= null">update_time,</if><if test="remark!= null">remark,</if>		</trim>	</sql>	<sql id="saveBatchValue">		<trim suffixOverrides=",">			<if test="item.id!= null">#{item.id},</if><if test="item.tableCode!= null">#{item.tableCode},</if><if test="item.type!= null">#{item.type},</if><if test="item.name!= null">#{item.name},</if><if test="item.code!= null">#{item.code},</if><if test="item.defaultVal!= null">#{item.defaultVal},</if><if test="item.orderNum!= null">#{item.orderNum},</if><if test="item.length!= null">#{item.length},</if><if test="item.clTableCode!= null">#{item.clTableCode},</if><if test="item.clColumnCode!= null">#{item.clColumnCode},</if><if test="item.primaryKey!= null">#{item.primaryKey},</if><if test="item.autoIncrement!= null">#{item.autoIncrement},</if><if test="item.notNull!= null">#{item.notNull},</if><if test="item.description!= null">#{item.description},</if><if test="item.deleted!= null">#{item.deleted},</if><if test="item.creator!= null">#{item.creator},</if><if test="item.createTime!= null">#{item.createTime},</if><if test="item.updator!= null">#{item.updator},</if><if test="item.updateTime!= null">#{item.updateTime},</if><if test="item.remark!= null">#{item.remark},</if>		</trim>	</sql>	<sql id="saveValue">		<trim suffixOverrides=",">			<if test="id!= null">#{id},</if><if test="tableCode!= null">#{tableCode},</if><if test="type!= null">#{type},</if><if test="name!= null">#{name},</if><if test="code!= null">#{code},</if><if test="defaultVal!= null">#{defaultVal},</if><if test="orderNum!= null">#{orderNum},</if><if test="length!= null">#{length},</if><if test="clTableCode!= null">#{clTableCode},</if><if test="clColumnCode!= null">#{clColumnCode},</if><if test="primaryKey!= null">#{primaryKey},</if><if test="autoIncrement!= null">#{autoIncrement},</if><if test="notNull!= null">#{notNull},</if><if test="description!= null">#{description},</if><if test="deleted!= null">#{deleted},</if><if test="creator!= null">#{creator},</if><if test="createTime!= null">#{createTime},</if><if test="updator!= null">#{updator},</if><if test="updateTime!= null">#{updateTime},</if><if test="remark!= null">#{remark},</if>		</trim>	</sql>	<delete id="deleteById" parameterType="com.cyber.domain.entity.TableColumn">		DELETE FROM table_column WHERE id = #{id}	</delete>    <delete id="deleteByTableCode">        DELETE FROM table_column WHERE table_code = #{tableCode}    </delete>    <update id="updateById" parameterType="com.cyber.domain.entity.TableColumn">		update table_column		<include refid="updateColumns" />		where id = #{id}	</update>	<select id="selectOne" resultType="com.cyber.domain.entity.TableColumn" parameterType="com.cyber.domain.entity.TableColumn">		SELECT		<include refid="baseColumns" />		FROM table_column        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>	</select>	<select id="selectByIndexCount" parameterType="com.cyber.domain.entity.TableColumn" resultType="int">		SELECT count(1)		FROM table_column        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>	</select>	<select id="selectByIndex" parameterType="com.cyber.domain.entity.TableColumn" resultType="com.cyber.domain.entity.TableColumn">		SELECT		<include refid="baseColumns" />		FROM table_column        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>		<if test="sortBy != null and sortBy != '' and sortType != null and sortType != '' ">			ORDER BY ${sortBy} ${sortType}		</if>		LIMIT #{offset} , #{limit}	</select></mapper>