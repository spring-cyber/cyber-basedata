<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.cyber.basedata.domain.repository.BaseDataMapper">    <sql id="baseColumns">        id        , parent_id, type, name, code,engine,collation,auto_increment_val, leader, description, deleted, creator, create_time, updator, update_time, remark    </sql>    <sql id="updateColumns">        <set>            <if test="id != null">                id = #{id},            </if>            <if test="parentId != null">                parent_id = #{parentId},            </if>            <if test="type != null">                type = #{type},            </if>            <if test="name != null">                name = #{name},            </if>            <if test="code != null">                code = #{code},            </if>            <if test="engine != null">                engine = #{engine},            </if>            <if test="collation != null">                collation = #{collation},            </if>            <if test="autoIncrementVal != null">                auto_increment_val = #{autoIncrementVal},            </if>            <if test="leader != null">                leader = #{leader},            </if>            <if test="description != null">                description = #{description},            </if>            <if test="deleted != null">                deleted = #{deleted},            </if>            <if test="creator != null">                creator = #{creator},            </if>            <if test="createTime != null">                create_time = #{createTime},            </if>            <if test="updator != null">                updator = #{updator},            </if>            <if test="updateTime != null">                update_time = #{updateTime},            </if>            <if test="remark != null">                remark = #{remark},            </if>        </set>    </sql>    <sql id="whereClause">        <trim prefix="WHERE" prefixOverrides="AND">            <if test="id != null">                AND ${columnPrefix}id = #{id}            </if>            <if test="parentId != null">                AND ${columnPrefix}parent_id = #{parentId}            </if>            <if test="type != null">                AND ${columnPrefix}type = #{type}            </if>            <if test="name != null">                AND ${columnPrefix}name = #{name}            </if>            <if test="code != null">                AND ${columnPrefix}code = #{code}            </if>            <if test="engine != null">                AND ${columnPrefix}engine = #{engine}            </if>            <if test="collation != null">                AND ${columnPrefix}collation = #{collation}            </if>            <if test="autoIncrementVal != null">                AND ${columnPrefix}auto_increment_val = #{autoIncrementVal}            </if>            <if test="leader != null">                AND ${columnPrefix}leader = #{leader}            </if>            <if test="description != null">                AND ${columnPrefix}description = #{description}            </if>            <if test="deleted != null">                AND ${columnPrefix}deleted = #{deleted}            </if>            <if test="creator != null">                AND ${columnPrefix}creator = #{creator}            </if>            <if test="createTime != null">                AND ${columnPrefix}create_time = #{createTime}            </if>            <if test="updator != null">                AND ${columnPrefix}updator = #{updator}            </if>            <if test="updateTime != null">                AND ${columnPrefix}update_time = #{updateTime}            </if>            <if test="remark != null">                AND ${columnPrefix}remark = #{remark}            </if>        </trim>    </sql>    <insert id="save" parameterType="com.cyber.domain.entity.BaseData">        insert into base_data        (<include refid="saveColumn"/>)        values        (<include refid="saveValue"/>)    </insert>    <sql id="saveColumn">        <trim suffixOverrides=",">            <if test="id!= null">id,</if>            <if test="parentId!= null">parent_id,</if>            <if test="type!= null">type,</if>            <if test="name!= null">name,</if>            <if test="code!= null">code,</if>            <if test="engine!= null">engine,</if>            <if test="collation!= null">collation,</if>            <if test="autoIncrementVal!= null">auto_increment_val,</if>            <if test="leader!= null">leader,</if>            <if test="description!= null">description,</if>            <if test="deleted!= null">deleted,</if>            <if test="creator!= null">creator,</if>            <if test="createTime!= null">create_time,</if>            <if test="updator!= null">updator,</if>            <if test="updateTime!= null">update_time,</if>            <if test="remark!= null">remark,</if>        </trim>    </sql>    <sql id="saveValue">        <trim suffixOverrides=",">            <if test="id!= null">#{id},</if>            <if test="parentId!= null">#{parentId},</if>            <if test="type!= null">#{type},</if>            <if test="name!= null">#{name},</if>            <if test="code!= null">#{code},</if>            <if test="engine!= null">#{engine},</if>            <if test="collation!= null">#{collation},</if>            <if test="autoIncrementVal!= null">#{autoIncrementVal},</if>            <if test="leader!= null">#{leader},</if>            <if test="description!= null">#{description},</if>            <if test="deleted!= null">#{deleted},</if>            <if test="creator!= null">#{creator},</if>            <if test="createTime!= null">#{createTime},</if>            <if test="updator!= null">#{updator},</if>            <if test="updateTime!= null">#{updateTime},</if>            <if test="remark!= null">#{remark},</if>        </trim>    </sql>    <delete id="deleteById" parameterType="com.cyber.domain.entity.BaseData">        DELETE        FROM base_data        WHERE id = #{id}    </delete>    <update id="updateById" parameterType="com.cyber.domain.entity.BaseData">        update base_data        <include refid="updateColumns"/>        where id = #{id}    </update>    <update id="executeSql">		<![CDATA[        ${changeSql}        ]]>	</update>    <select id="selectOne" resultType="com.cyber.domain.entity.BaseData"            parameterType="com.cyber.domain.entity.BaseData">        SELECT        <include refid="baseColumns"/>,u.name leaderName        from base_data bd        left join cyber_auth.user u on bd.leader = u.id        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>    </select>    <select id="selectByIndexCount" parameterType="com.cyber.domain.entity.BaseData" resultType="int">        SELECT count(1)        FROM base_data        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>    </select>    <select id="selectByIndex" parameterType="com.cyber.domain.entity.BaseData"            resultType="com.cyber.domain.entity.BaseData">        SELECT        <include refid="baseColumns"/>        FROM base_data        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>        <if test="sortBy != null and sortBy != '' and sortType != null and sortType != '' ">            ORDER BY ${sortBy} ${sortType}        </if>        LIMIT #{offset} , #{limit}    </select>    <select id="selectBaseData" resultType="com.cyber.domain.entity.BaseData">        SELECT        <include refid="baseColumns"/>        FROM base_data        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>        <if test="sortBy != null and sortBy != '' and sortType != null and sortType != '' ">            ORDER BY ${sortBy} ${sortType}        </if>    </select>    <select id="checkBaseDataNameUnique" resultType="com.cyber.domain.entity.BaseData">        select id, name        from base_data        where name = #{name}          and type = #{type}          and deleted = 0 limit 1    </select>    <select id="checkBaseDataCodeUnique" resultType="com.cyber.domain.entity.BaseData">        select id, code        from base_data        where code = #{code}          and type = #{type}          and deleted = 0 limit 1    </select>    <select id="hasChildByBaseDataId" resultType="java.lang.Integer">        select count(1)        from base_data        where parent_id = #{baseDataId}    </select>    <select id="hasTableForDatabase" resultType="java.lang.Object">        SHOW        TABLES LIKE        #{code};    </select>    <select id="selectTableDataCount" resultType="java.lang.Integer">        SELECT count(1) FROM ${tableCode}    </select>    <select id="selectTableDataByIndex" resultType="com.alibaba.fastjson.JSONObject">        SELECT * FROM ${tableCode}        <if test="sortBy != null and sortBy != '' and sortType != null and sortType != '' ">            ORDER BY ${sortBy} ${sortType}        </if>        LIMIT #{offset} , #{limit}    </select></mapper>