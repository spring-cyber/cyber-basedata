 <if test="name != null">	    AND ${columnPrefix}name = #{name}  </if>  <if test="landmark != null">	    AND ${columnPrefix}landmark = #{landmark}  </if>  <if test="accuracy != null">	    AND ${columnPrefix}accuracy = #{accuracy}  </if>  <if test="wholeName != null">	    AND ${columnPrefix}whole_name = #{wholeName}  </if>  <if test="lon != null">	    AND ${columnPrefix}lon = #{lon}  </if>  <if test="lat != null">	    AND ${columnPrefix}lat = #{lat}  </if>  <if test="cityCode != null">	    AND ${columnPrefix}city_code = #{cityCode}  </if>  <if test="zipCode != null">	    AND ${columnPrefix}zip_code = #{zipCode}  </if>  <if test="areaCode != null">	    AND ${columnPrefix}area_code = #{areaCode}  </if>  <if test="pinYin != null">	    AND ${columnPrefix}pin_yin = #{pinYin}  </if>  <if test="simplePy != null">	    AND ${columnPrefix}simple_py = #{simplePy}  </if>  <if test="perPinYin != null">	    AND ${columnPrefix}per_pin_yin = #{perPinYin}  </if>  <if test="creator != null">	    AND ${columnPrefix}creator = #{creator}  </if>  <if test="createTime != null">	    AND ${columnPrefix}create_time = #{createTime}  </if>  <if test="updator != null">	    AND ${columnPrefix}updator = #{updator}  </if>  <if test="updateTime != null">	    AND ${columnPrefix}update_time = #{updateTime}  </if>        </trim>    </sql>	<insert id="save" parameterType="com.cyber.domain.entity.Area">		insert into area		(<include refid="saveColumn"/>)		values		(<include refid="saveValue"/>)	</insert>	<sql id="saveColumn">		<trim suffixOverrides=",">			<if test="id!= null">id,</if><if test="parentId!= null">parent_id,</if><if test="level!= null">level,</if><if test="name!= null">name,</if><if test="landmark!= null">landmark,</if><if test="accuracy!= null">accuracy,</if><if test="wholeName!= null">whole_name,</if><if test="lon!= null">lon,</if><if test="lat!= null">lat,</if><if test="cityCode!= null">city_code,</if><if test="zipCode!= null">zip_code,</if><if test="areaCode!= null">area_code,</if><if test="pinYin!= null">pin_yin,</if><if test="simplePy!= null">simple_py,</if><if test="perPinYin!= null">per_pin_yin,</if><if test="creator!= null">creator,</if><if test="createTime!= null">create_time,</if><if test="updator!= null">updator,</if><if test="updateTime!= null">update_time,</if>		</trim>	</sql>	<sql id="saveValue">		<trim suffixOverrides=",">			<if test="id!= null">#{id},</if><if test="parentId!= null">#{parentId},</if><if test="level!= null">#{level},</if><if test="name!= null">#{name},</if><if test="landmark!= null">#{landmark},</if><if test="accuracy!= null">#{accuracy},</if><if test="wholeName!= null">#{wholeName},</if><if test="lon!= null">#{lon},</if><if test="lat!= null">#{lat},</if><if test="cityCode!= null">#{cityCode},</if><if test="zipCode!= null">#{zipCode},</if><if test="areaCode!= null">#{areaCode},</if><if test="pinYin!= null">#{pinYin},</if><if test="simplePy!= null">#{simplePy},</if><if test="perPinYin!= null">#{perPinYin},</if><if test="creator!= null">#{creator},</if><if test="createTime!= null">#{createTime},</if><if test="updator!= null">#{updator},</if><if test="updateTime!= null">#{updateTime},</if>		</trim>	</sql>	<delete id="deleteById" parameterType="com.cyber.domain.entity.Area">		DELETE FROM area WHERE id = #{id}	</delete>	<update id="updateById" parameterType="com.cyber.domain.entity.Area">		update area		<include refid="updateColumns" />		where id = #{id}	</update>	<select id="selectOne" resultType="com.cyber.domain.entity.Area" parameterType="com.cyber.domain.entity.Area">		SELECT		<include refid="baseColumns" />		FROM area        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>	</select>	<select id="selectByIndexCount" parameterType="com.cyber.domain.entity.Area" resultType="int">		SELECT count(1)		FROM area        <include refid="whereClause">    